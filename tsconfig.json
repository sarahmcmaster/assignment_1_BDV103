{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node 20",
  "_version": "20.1.0",

"compilerOptions": {
  "lib": ["es2023"],
  "target": "es2022",

  "module": "commonjs",
  "moduleResolution": "node",
  "types": ["node"],

  "strict": true,
  "esModuleInterop": true,
  "skipLibCheck": true,
  "forceConsistentCasingInFileNames": true,
  "resolveJsonModule": true
}
}
